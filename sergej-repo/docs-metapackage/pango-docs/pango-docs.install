_print_msg() {
  /bin/cat << EOF

===> pango-docs post-install message:

===> If you'd like to have cross references right in the installed
===> HTML pages, you should install gtk2-docs and glib2-docs first
===> and rebuild this package.

EOF
}

post_install() {
  _print_msg
}

post_upgrade() {
  _print_msg
}
